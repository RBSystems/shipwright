{
  "caches": [
    {
      "name": "default",
      "storage-type": "elk",
      "cache-type": "redis",
      "elk-cache": {
        "device-index": "oit-static-av-devices-test-1",
        "room-index": "oit-static-av-rooms-test",
        "url": "http://av-elk-rapidmaster1:9200"
      },
      "redis-cache": {
          "device-database": 0,
          "room-database": 1,
          "url": "10.0.12.115:6379"
      }
    }
  ],
  "forwarders": [
    {
      "name": "MainStaticDeviceIndex",
      "type": "elkstatic",
      "event-type": "all",
      "interval": 15,
      "data-type": "device",
      "cache-name": "default",
      "elk": {
        "url": "http://av-elk-rapidmaster1:9200",
        "index-pattern": "oit-static-av-devices-test-1",
        "index-rotation-interval": "norotate",
        "upsert": true
      }
    },
    {
      "name": "MainStaticRoom",
      "type": "elkstatic",
      "event-type": "all",
      "interval": 15,
      "data-type": "room",
      "cache-name": "default",
      "elk": {
        "url": "http://av-elk-rapidmaster1:9200",
        "index-pattern": "oit-static-av-rooms-test",
        "index-rotation-interval": "norotate",
        "upsert": true
      }
    }
  ]
}
